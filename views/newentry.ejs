<%- include ('partials/header.ejs'); %>
<%- include ('partials/navbar.ejs'); %>

<div class="container">
  <div class="card-header">
    <h2>New entry</h2>
  </div>
  <div class="card-body">
    <form method="POST" action="">
      <label>Plate</label>
      <input type="text" name="plate" placeholder="ABC-1234" class="form-control">
      <br>
      <label>Manufacturer</label>
      <input type="text" name="manufacturer" placeholder="Chrysler" class="form-control">
      <br>
      <label>Model</label>
      <input type="text" name="Model" placeholder="300c" class="form-control">
      <br>
      <label>Entry date</label>
      <input type="date" name="entrydate" class="form-control">
      <br>
      <label>Entry hour</label>
      <input type="time" name="entryhour" class="form-control">
      <br>
      <button class="btn btn-success">Submit</button>
      <br>
    </form>
  </div>
</div>

<%- include ('partials/footer.ejs'); %>